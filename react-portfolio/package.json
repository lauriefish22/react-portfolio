{
    "name": "react-portfolio",
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceRoot}/src"
        }
    ],
    "private": true,
    "dependencies": {
        "axios": "^1.4.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "bootstrap": "^5.2.3",
        "dotenv": "^16.0.3",
        "react": "^17.0.2",
        "formik": "^2.4.0",
        "react-bootstrap": "^2.4.0",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.1",
        "reactstrap": "^9.1.10",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
